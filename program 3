insert into employee values(4,'anjali','cs','12-jun-2010','15-july-1985',20000);
insert into employee values(5,'rohan','IT','27-jul-2009','10-aug-1982',35000);
insert into employee values(6,'jatin','cs','27-jul-2009','10-aug-1982',40000);
select *from employee  ;
select *from employee
where dept = 'mca'; 
select id,name,dept from employee;

select id,name,dept from employee
where salary = 20000;
