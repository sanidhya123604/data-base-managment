create table employee
(
id number(5),
name varchar(20),
dept varchar(20),
doj date,
dob date,
salary number(10)
);
insert into employee values(1,'ram kumar','mca','27-jul-2009','15-aug-1982',25000);
insert into employee values(2,'ram amain jain','mca','27-jul-2008','15-aug-1985',20000);
